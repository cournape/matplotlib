Name: matplotlib
Version: 1.1.0
Summary: Python plotting package
Url: http://matplotlib.sourceforge.net
Description:     
          matplotlib strives to produce publication quality 2D graphics
          for interactive graphing, scientific publishing, user interface
          development and web application servers targeting multiple user
          interfaces and hardcopy output formats.  There is a 'pylab' mode
          which emulates matlab graphics
Author: John D. Hunter
AuthorEmail: jdh2358@gmail.com
Maintainer: John D. Hunter
MaintainerEmail: jdh2358@gmail.com
Platforms: any

DataFiles: gendata_matplotlib
    SourceDir: .
    TargetDir: $sitedir/matplotlib
    Files:
        matplotlib/mpl-data/fonts/afm/*.afm,
        matplotlib/mpl-data/fonts/pdfcorefonts/*.afm,
        matplotlib/mpl-data/fonts/pdfcorefonts/*.txt,
        matplotlib/mpl-data/fonts/ttf/*.ttf,
        matplotlib/mpl-data/fonts/ttf/LICENSE_STIX,
        matplotlib/mpl-data/fonts/ttf/COPYRIGHT.TXT,
        matplotlib/mpl-data/fonts/ttf/README.TXT,
        matplotlib/mpl-data/fonts/ttf/RELEASENOTES.TXT,
        matplotlib/mpl-data/images/*.xpm,
        matplotlib/mpl-data/images/*.svg,
        matplotlib/mpl-data/images/*.png,
        matplotlib/mpl-data/images/*.ppm,
        matplotlib/mpl-data/matplotlibrc,
        matplotlib/mpl-data/matplotlib.conf,
        matplotlib/mpl-data/*.glade,
        matplotlib/backends/Matplotlib.nib/*

HookFile: bscript

